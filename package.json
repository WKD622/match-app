      ],
      "^@stores(.*)$": [
        "<rootDir>/src/stores/$1"
      ]
    }
  },
  "transform": {
    "\\.js$": "<rootDir>/node_modules/babel-jest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
